VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
# Server-side Supabase admin key (Vercel Functions)
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
# Optional: override magic link invite redirect URL
VITE_INVITE_REDIRECT_URL=https://your-app-domain.com/
# Server-side key for Vercel Functions (Do not expose in Vite)
GEMINI_API_KEY=your_google_gemini_api_key
# Replicate video generation (server-side only)
REPLICATE_API_TOKEN=your_replicate_api_token
# Replicate model (either model slug or version)
REPLICATE_VIDEO_MODEL=google/veo-3.1
# Optional: pin a specific model version ID
REPLICATE_VIDEO_VERSION=your_replicate_model_version
# Optional: override input keys used by the model
REPLICATE_VIDEO_IMAGE_KEY=reference_images
REPLICATE_VIDEO_PROMPT_KEY=prompt
# Optional JSON defaults passed into the model input
REPLICATE_VIDEO_INPUT_DEFAULTS={"fps":6,"num_frames":14}
# AI video rate limit (per user, per window)
AI_VIDEO_RATE_LIMIT_MAX=5
AI_VIDEO_RATE_LIMIT_WINDOW_MINUTES=60
# Enable logging unknown AI queries into ai_query_pool
VITE_ENABLE_QUERY_POOL=false
# Enable DB-backed FAQ library
VITE_ENABLE_FAQ_DB=true
# FAQ cache TTL (ms)
VITE_FAQ_CACHE_TTL_MS=300000
# Max FAQ rows to load into memory
VITE_FAQ_MAX_ENTRIES=400
# Vector similarity threshold for FAQ matches
VITE_FAQ_MIN_SCORE=0.48
# Capture LLM fallback answers into FAQ: off | draft | approved
VITE_FAQ_CAPTURE_MODE=draft
# Legacy (Removed)
# VITE_GEMINI_API_KEY=deprecated

# Automation Webhook (n8n or Make)
VITE_N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/facebook-post-trigger
