VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
# Optional: override magic link invite redirect URL
VITE_INVITE_REDIRECT_URL=https://your-app-domain.com/
# Server-side key for Vercel Functions (Do not expose in Vite)
GEMINI_API_KEY=your_google_gemini_api_key
# Enable logging unknown AI queries into ai_query_pool
VITE_ENABLE_QUERY_POOL=false
# Enable DB-backed FAQ library
VITE_ENABLE_FAQ_DB=true
# FAQ cache TTL (ms)
VITE_FAQ_CACHE_TTL_MS=300000
# Max FAQ rows to load into memory
VITE_FAQ_MAX_ENTRIES=400
# Vector similarity threshold for FAQ matches
VITE_FAQ_MIN_SCORE=0.48
# Capture LLM fallback answers into FAQ: off | draft | approved
VITE_FAQ_CAPTURE_MODE=draft
# Legacy (Removed)
# VITE_GEMINI_API_KEY=deprecated

# Automation Webhook (n8n or Make)
VITE_N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/facebook-post-trigger
